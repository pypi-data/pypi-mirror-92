{
  "name": "jupyterlab-s3-browser",
  "version": "0.8.0-dev.6",
  "description": "JupyterLab extension for browsing S3-compatible object storage",
  "keywords": [
    "s3",
    "cloud",
    "object",
    "storage",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/IBM/jupyterlab-s3-browser",
  "bugs": {
    "url": "https://github.com/IBM/jupyterlab-s3-browser/issues"
  },
  "license": "Apache-2.0",
  "author": "James Reeve",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM/jupyterlab-s3-browser.git"
  },
  "style": "style/index.css",
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "genversion --es6 --semi src/version.ts && tsc",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "clean": "rimraf lib",
    "precommit": "lint-staged",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "test": "cd test && ./run-tests.sh",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/apputils": "^3.0.0",
    "@jupyterlab/coreutils": "^5.0.0",
    "@jupyterlab/docmanager": "^3.0.0",
    "@jupyterlab/docregistry": "^3.0.0",
    "@jupyterlab/filebrowser": "^3.0.0",
    "@jupyterlab/services": "^6.0.0",
    "@jupyterlab/settingregistry": "^3.0.0",
    "@lumino/algorithm": "^1.2.3",
    "@lumino/messaging": "^1.3.3",
    "@lumino/signaling": "^1.3.5",
    "@lumino/widgets": "^1.17.0",
    "base64-js": "^1.2.3",
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.1",
    "@types/base64-js": "^1.2.5",
    "@types/text-encoding": "^0.0.32",
    "genversion": "^2.2.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "subpackage": "^1.1.0",
    "typescript": "~4.1.3"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "@types/react": "^17.0.0"
  },
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyterlab_s3_browser"
        }
      }
    },
    "outputDir": "jupyterlab_s3_browser/labextension"
  }
}
