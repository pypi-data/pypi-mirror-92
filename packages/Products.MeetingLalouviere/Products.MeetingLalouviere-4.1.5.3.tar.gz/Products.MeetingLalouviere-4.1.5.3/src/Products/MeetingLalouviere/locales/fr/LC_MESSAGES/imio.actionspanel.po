msgid "setInCommittee_done_descr"
msgstr "La séance est passée en commission."

#. Default: "setInCouncil_done_descr"
msgid "setInCouncil_done_descr"
msgstr "La séance est passée en Conseil communal."

#. Default: "setItemInCommittee_done_descr"
