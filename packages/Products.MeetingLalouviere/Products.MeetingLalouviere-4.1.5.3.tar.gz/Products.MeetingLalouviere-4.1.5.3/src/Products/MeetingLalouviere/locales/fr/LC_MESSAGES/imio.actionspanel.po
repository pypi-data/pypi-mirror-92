msgstr "Le point a été envoyé au chef de division."

#. Default: "proposeToOfficeManager_done_descr"
msgid "proposeToOfficeManager_done_descr"
msgstr "Le point a été envoyé au chef de bureau."

#. Default: "proposeToServiceHead_done_descr"
msgid "proposeToServiceHead_done_descr"
msgstr "Le point a été envoyé au chef de service."

#. Default: "refuse_done_descr"
msgid "refuse_done_descr"
msgstr "Le point a été refusé."

#. Default: "return_to_secretary_done_descr"
msgid "return_to_secretary_done_descr"
msgstr "Le point a été renvoyé au Secrétariat communal."

#. Default: "return_to_secretary_in_committee_done_descr"
msgid "return_to_secretary_in_committee_done_descr"
msgstr "Le point a été renvoyé au Secrétariat communal."

#. Default: "return_to_secretary_in_council_done_descr"
msgid "return_to_secretary_in_council_done_descr"
msgstr "Le point a été renvoyé au Secrétariat communal."

