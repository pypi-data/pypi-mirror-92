<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_create_subscription" model="ir.ui.view">
            <field name="name">Create Subscription</field>
            <field name="model">partner.create.subscription</field>
            <field name="inherit_id" ref="easy_my_coop.view_create_subscription" />
            <field name="arch" type="xml">
              <field name="bank_account" position="attributes">
                <attribute name="invisible">1</attribute>
              </field>
              <field name="bank_account" position="after">
                <field name="bank_id" domain="[('partner_id', '=', cooperator)]"/>
                <field name="payment_type" />
              </field>
              <field name="share_product" position="attributes">
                <attribute name="invisible">1</attribute>
              </field>
              <field name="share_qty" position="attributes">
                <attribute name="invisible">1</attribute>
              </field>
              <field name="share_unit_price" position="attributes">
                <attribute name="invisible">1</attribute>
              </field>
            </field>
        </record>
    </data>
</odoo>