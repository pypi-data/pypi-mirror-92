    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.newDream'"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.newDream/less/mobilelarge.less" />

