<?xml version="1.0"?>
<object name="portal_less">

 <stylesheet title="CPSkin LESS Variables"
    id="++theme++cpskin.theme.base/less/variables.less"
    media=""
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    compression="none" />

 <stylesheet title="CPSkin LESS Custom Variables"
    id="portal_resources/cpskin/variables.less"
    media=""
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    compression="none"
    insert-after="++theme++cpskin.theme.base/less/variables.less" />

 <stylesheet title="CPSkin LESS Styles"
    id="++theme++cpskin.theme.base/less/styles.less"
    media="all"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    compression="none"
    insert-after="portal_resources/cpskin/variables.less" />
 
 <stylesheet title="CPSkin LESS Contact"
    id="++theme++cpskin.theme.base/less/contact.less"
    media=""
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    compression="none"
    insert-after="++theme++cpskin.theme.base/less/styles.less" />

 <stylesheet title="CPSkin LESS Mobile Styles"
    id="++theme++cpskin.theme.base/less/mobile.less"
    media="handheld, screen and (max-device-width: 480px), screen and (max-width: 480px)"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    compression="none"
    insert-after="++theme++cpskin.theme.base/less/contact.less" />

 <stylesheet title="CPSkin LESS Medium Mobile Styles"
    id="++theme++cpskin.theme.base/less/mobilemedium.less"
    media="handheld, screen and (min-device-width: 481px) and (max-device-width: 768px), screen and(min-width: 481px) and (max-width: 768px)"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    compression="none"
    insert-after="++theme++cpskin.theme.base/less/mobile.less" />

 <stylesheet title="CPSkin LESS Large Mobile Styles"
    id="++theme++cpskin.theme.base/less/mobilelarge.less"
    media="handheld, screen and (min-device-width: 769px) and (max-device-width: 1024px), screen and(min-width: 769px) and (max-width: 1024px)"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    compression="none"
    insert-after="++theme++cpskin.theme.base/less/mobilemedium.less" />

 <stylesheet title="CPSkin LESS Custom Styles"
    id="portal_resources/cpskin/styles.less"
    media=""
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    compression="none"
    insert-after="++theme++cpskin.theme.base/less/mobilelarge.less" />

</object>
