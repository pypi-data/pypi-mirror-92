from unittest import TestCase

from crate_anon.nlp_manager.base_nlp_parser import BaseNlpParser


class FruitParser(BaseNlpParser):
    pass


class NlpParserTests(TestCase):
    def test_process_inserts_values(self) -> None:
        starting_fields_values = {}

        parser = FruitParser()

        parser.process(
            "Apple Banana Courgette Dandelion Eggplant Fig",
            starting_fields_values
        )
